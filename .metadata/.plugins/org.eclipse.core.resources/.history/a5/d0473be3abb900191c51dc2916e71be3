package beeptask;

import java.awt.Toolkit;

public class BeepPrintLambdaEx {
	public static void main(String[] args) {
		Thread thread = new Thread(()->{
			Toolkit tookit = Toolkit.getDefaultToolkit();
			for (int i = 0; i < 5; i++) {
				tookit.beep();
				System.out.println("ë¹ž");
				try {
					Thread.sleep(500);
				} catch (Exception e) {
				}
			}
		});
		thread.start();
	}
}
